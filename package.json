{
  "name": "nuxt3-awesome-starter",
  "version": "0.1.0",
  "description": "a Nuxt 3 starter template or boilerplate with a lot of useful features. Nuxt3 + Tailwindcss 3",
  "repository": {
    "type": "git",
    "url": "git://github.com/viandwi24/nuxt3-awesome-starter"
  },
  "author": "viandwi24",
  "license": "MIT",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "node .output/server/index.mjs",
    "postinstall": "husky install",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "test": "jest",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@headlessui/vue": "^1.4.3",
    "@iconify/json": "^1.1.456",
    "@intlify/nuxt3": "^0.1.10",
    "@nuxtjs/eslint-config-typescript": "^7.0.2",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/svg": "^0.3.0",
    "@pinia/nuxt": "^0.1.8",
    "@types/jest": "^27.4.0",
    "@vue/compiler-sfc": "^3.2.29",
    "@vue/test-utils": "^2.0.0-rc.18",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.3",
    "nuxt3": "^3.0.0-27389502.88da719",
    "pinia": "^2.0.9",
    "postcss": "^8.4.5",
    "prettier": "^2.5.0",
    "sass": "^1.47.0",
    "sass-loader": "^12.4.0",
    "tailwind-config-viewer": "^1.6.3",
    "tailwindcss": "^3.0.13",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.2",
    "unplugin-icons": "^0.13.0",
    "unplugin-vue-components": "^0.17.11",
    "vue-jest": "^5.0.0-alpha.10",
    "vue3-jest": "^27.0.0-alpha.1"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html,css}": [
      "yarn lintfix"
    ]
  }
}
